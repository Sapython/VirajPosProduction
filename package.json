{
  "name": "virajpos",
  "version": "1.0.41",
  "description": "Vrajera POS system.",
  "homepage": "https://virajworld.in/",
  "author": {
    "name": "Shreeva",
    "email": "create@shreeva.com"
  },
  "repository": "github:swayambhu-innovations/Vrajera",
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "prebuild": "npm --no-git-tag-version version patch",
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "ng:serve": "ng serve -c web -o",
    "ng:serve:external": "ng serve -c web -o --host 192.168.43.183",
    "build": "npm run electron:serve-tsc && ng build --aot --base-href ./ --stats-json",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "analyze:build": "npm run build && npm run analyze",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "web:build": "npm run build -- -c web-production",
    "electron": "electron",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "electron:build:only": "electron-builder build --publish=never",
    "electron:build:publish": "npm run build:prod && electron-builder build --publish=always",
    "electron:publish:only": "electron-builder build --publish=always",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "pretty": "prettier --write \"./**/*.{ts,json,html,scss}\"",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run start"
  },
  "dependencies": {
    "@angular/animations": "17.3.12",
    "@angular/cdk": "~17.3.10",
    "@angular/common": "17.3.12",
    "@angular/compiler": "17.3.12",
    "@angular/core": "17.3.12",
    "@angular/fire": "17.1.0",
    "@angular/forms": "17.3.12",
    "@angular/language-service": "17.3.12",
    "@angular/material": "~17.3.10",
    "@angular/material-moment-adapter": "17.3.10",
    "@angular/platform-browser": "17.3.12",
    "@angular/platform-browser-dynamic": "17.3.12",
    "@angular/router": "17.3.12",
    "@angular/service-worker": "17.3.12",
    "@types/hammerjs": "2.0.41",
    "angular-animations": "0.11.0",
    "chart.js": "4.3.0",
    "fuse.js": "6.6.2",
    "hammerjs": "2.0.8",
    "jspdf": "2.5.1",
    "jspdf-autotable": "3.5.28",
    "moment": "2.29.4",
    "ngx-indexed-db": "16.0.0",
    "remixicon": "3.2.0",
    "rxjs": "7.8.0",
    "tslib": "2.8.1",
    "xlsx": "0.18.5",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "17.0.2",
    "@angular-devkit/build-angular": "17.3.11",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "17.3.6",
    "@angular/compiler-cli": "17.3.12",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "codelyzer": "6.0.2",
    "conventional-changelog-cli": "2.2.2",
    "electron": "30.0.1",
    "electron-builder": "24.13.3",
    "electron-debug": "3.2.0",
    "electron-reloader": "1.2.3",
    "eslint": "8.34.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.2.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "node-polyfill-webpack-plugin": "2.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.4.5",
    "wait-on": "7.0.1",
    "webdriver-manager": "12.1.8",
    "webpack-bundle-analyzer": "12.1.9"
  },
  "engines": {
    "node": ">= 16.14.0 || >= 18.10.0"
  },
  "browserslist": [
    "chrome 114"
  ]
}
